# BEGIN WordPress
<IfModule mod_rewrite.c>
    RewriteEngine on
    RewriteCond %{REQUEST_FILENAME} -s [OR]
    RewriteCond %{REQUEST_FILENAME} -l [OR]
    RewriteCond %{REQUEST_FILENAME} -d
    RewriteRule ^(.*)$ - [NC,L]
    RewriteRule ^(.*)\.tpl\.html$ /index.php/$1 [NC,L]

    RewriteRule ^(.*) /index.php [NC,L]
</IfModule>

# END WordPress
